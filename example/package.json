{
  "name": "expo-object-tracker-example",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "delete:module": "rm -rf package-lock.json && rm -rf yarn.lock && rm -rf node_modules",
    "delete:pods": "rm -rf ios/Podfile.lock && rm -rf ios/Pods",
    "install:pods": "cd ios && pod repo update && pod update && pod install"
  },
  "dependencies": {
    "expo": "~53.0.22",
    "react": "19.0.0",
    "react-native": "0.79.6",
    "expo-document-picker": "~13.1.6",
    "expo-file-system": "~18.1.11",
    "expo-video-thumbnails": "~9.1.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "expo-asset": "~11.1.7",
    "expo-linking": "~7.1.7",
    "@shopify/react-native-skia": "v2.0.0-next.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "private": true,
  "expo": {
    "autolinking": {
      "nativeModulesDir": ".."
    }
  }
}
